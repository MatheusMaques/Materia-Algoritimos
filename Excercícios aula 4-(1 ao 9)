programa
{
	//4-Escrever um algoritmo que leia o nome e as três notas obtidas por um aluno durante o semestre. Calcular a sua média (aritmética), informar o nome e sua menção aprovado (media >= 7), Reprovado (media <= 5) e Recuperação (media entre 5.1 a 6.9). 
	inclua biblioteca Matematica
	funcao inicio()
	{
		//variaveis
		cadeia aluno
		real n1, n2, n3, media

		escreva ("Digite o nome do aluno: ", "\n")
			leia (aluno)
		escreva ("Informe a primeira nota: ", "\n")
			leia (n1)
		escreva ("Iforme a segunda nota: ", "\n")
			leia (n2)
		escreva ("Informe a terceira nota: ", "\n")
			leia (n3)

		media = (n1 + n2 + n3) / 3
		media =Matematica.arredondar(media,2)

		se (media >= 7)
			escreva ("O aluno ", aluno, " está APROVADO,com média ", media)
		senao se (media <= 5)
			escreva ("O aluno ", aluno, " está REPROVADO,com média ", media)
		senao se (media >= 5.1 e media <= 6.9)
			escreva ("O aluno ", aluno, " está de RECUPERAÇÃO,com média ", media)
	}
}

