programa
{
	
//1-Faça um algoritmo que receba um número e mostre uma mensagem caso este número seja maior que 10. 

	funcao inicio()
	{
		//variaveis
		inteiro x = 0
 
		escreva (("Digite um número: "), "\n")
			leia(x)

		//verificação do valor do número informado
		se (x >= 10){
			escreva ("Este número é maior que 10")
		}
		senao {
			escreva ("Este número é menor que 10")
		}
		
		
	}
}
