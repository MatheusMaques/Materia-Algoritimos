programa
{
	
//8- Faça um algoritmo que receba o número do mês e mostre o mês correspondente. Valide mês inválido. 
	funcao inicio()
	{
		//variaveis
		inteiro nmes
		
		escreva ("Informe o número do mês que estamos: ", "\n")
			leia(nmes)
			
		enquanto (nmes > 12){
			escreva ("Erro, informe um número válido:", "\n")
			 	leia (nmes)
			 	}
		
		escolha (nmes){
			caso 1:
			escreva ("Estamos no mês de Janeiro")
			pare
			
			caso 2:
			escreva ("Estamos no mês de Fevereiro")
			pare
			
			caso 3:
			escreva ("Estamos no mês de Março")
			pare
			
			caso 4:
			escreva ("Estamos no mês de Abril")
			pare
			
			caso 5:
			escreva ("Estamos no mês de Maio")
			pare

			caso 6:
			escreva ("Estamos no mês de Junho")
			pare

			caso 7:
			escreva ("Estamos no mês de Julho")
			pare

			caso 8:
			escreva ("Estamos no mês de Agosto")
			pare

			caso 9:
			escreva("Estamos no mês de Setembro")
			pare

			caso 10:
			escreva ("Estamos no mês de Outubro")
			pare

			caso 11:
			escreva ("Estamos no mês de Novembro")
			pare

			caso 12:
			escreva ("Estamos no mês de Dezembro")
			pare
		}
	}
}

