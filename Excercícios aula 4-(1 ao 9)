programa
{
	//7-Faça um algoritmo que leia um número de 1 a 5 e escreva por extenso. Caso o usuário digite um número que não esteja neste intervalo, exibir mensagem: número inválido.	
	funcao inicio()
	{
		//variaveis
		inteiro x

		escreva ("Informe um número de 1 a 5 ", "\n")
			leia(x)
			
		enquanto (x > 5){
			escreva ("Erro, informe um número válido:", "\n")
			 	leia (x)
			 	}
		
		escolha (x){
			caso 1:
			escreva ("O número informado foi o número um")
			pare
			
			caso 2:
			escreva ("O número informado foi o número dois")
			pare
			
			caso 3:
			escreva ("O número informado foi o número três")
			pare
			
			caso 4:
			escreva ("O número informado foi o número quatro")
			pare
			
			caso 5:
			escreva ("O número informado foi o número cinco")
			pare
		}
	}
}

