programa
{
	
//3-Faça um algoritmo que receba um número e diga se este número está no intervalo entre 100 e 200.
	funcao inicio()
	{
		//variaveis
		inteiro x

		escreva ("Digite um valor: ", "\n")
			leia (x)

		se (x >=100 e x <= 200)
			escreva ("O valor de X está entre 100 e 200, sendo ", x)
		senao 
			escreva ("O valor de X não está entre 100 e 200")
	}
}

