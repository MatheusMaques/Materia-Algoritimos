programa
{
	//9- Faça um algoritmo que receba o nome a idade, o sexo e salário fixo de um funcionário. Mostre o nome e o salário líquido: 
	funcao inicio()
	{
	//variaveis
	cadeia nome
	inteiro idade
	caracter sexo
	real sbruto, sliquido, inss, irrf, sli 

		escreva ("         FICHA CADASTRAL FUNCIONARIOS     ", "\n")
		escreva ("Nome: ", "\n")
			leia (nome)
		escreva ("Idade: ", "\n")
			leia (idade)
		escreva ("Sexo: ", "\n")
			leia (sexo)
		escreva ("Salário Bruto: ", "\n")
			leia(sbruto)

		//Calculo do inss até R$1302,isento de imposto de renda
		se (sbruto <= 1302){
			inss = (sbruto * 7.5) / 100
			sliquido = sbruto - inss
				
		escreva ("          INFORMAÇÕES FUNCIONÁRIOS       ", "\n")
		escreva ("Nome: ", nome, "\n")
		escreva ("Idade: ", idade, "\n")
		escreva ("Sexo: ", sexo, "\n")
		escreva ("Salário líquido: ", sliquido)
		}
////////////////////////////////////////////////////////////////////////////////////////////
		//Calculo do inss de R$1303 a R$2571
		se (sbruto >= 1303 e sbruto <= 2571){
			inss = (sbruto * 9) / 100
			sliquido = sbruto - inss

			//Calculo do imposto de renda de R$1904 a R$2827
			se (sbruto >= 1904 e sbruto <= 2827){
				irrf = (sbruto * 7.5) / 100
				sliquido = sbruto - inss - irrf	
			}

			//calculo do imposto de renda de R$2.828 a R$3.751
			se (sbruto >= 2828 e sbruto <= 3751){
				irrf = (sbruto * 15) / 100
				sliquido = sbruto - inss - irrf	
			}

			//calculo do imposto de renda de R$3.752 a R$4.665
			se (sbruto >= 3752 e sbruto <= 4665){
				irrf = (sbruto * 22.5) / 100
				sliquido = sbruto - inss - irrf	
			}

			
			//calculo do imposto de renda de R$4666 para cima 
			se (sbruto >= 4666){
				irrf = (sbruto * 27.5) / 100
				sliquido = sbruto - inss - irrf	
			}
			
		escreva ("          INFORMAÇÕES FUNCIONÁRIOS       ", "\n")
		escreva ("Nome: ", nome, "\n")
		escreva ("Idade: ", idade, "\n")
		escreva ("Sexo: ", sexo, "\n")
		escreva ("Salário líquido: ", sliquido)
		}
////////////////////////////////////////////////////////////////////////////////////////////
		//calculo do inss de R$2752 à R$3857
		se (sbruto >= 2572 e sbruto <= 3857){
			inss = (sbruto * 12) / 100
			sliquido = sbruto - inss

			//Calculo do imposto de renda de R$1904 a R$2827
			se (sbruto >= 1904 e sbruto <= 2827){
				irrf = (sbruto * 7.5) / 100
				sliquido = sbruto - inss - irrf	
			}

			//calculo do imposto de renda de R$2.828 a R$3.751
			se (sbruto >= 2828 e sbruto <= 3751){
				irrf = (sbruto * 15) / 100
				sliquido = sbruto - inss - irrf	
			}

			//calculo do imposto de renda de R$3.752 a R$4.665
			se (sbruto >= 3752 e sbruto <= 4665){
				irrf = (sbruto * 22.5) / 100
				sliquido = sbruto - inss - irrf	
			}

			
			//calculo do imposto de renda de R$4666 para cima 
			se (sbruto >= 4666){
				irrf = (sbruto * 27.5) / 100
				sliquido = sbruto - inss - irrf	
			}
			
		escreva ("          INFORMAÇÕES FUNCIONÁRIOS       ", "\n")
		escreva ("Nome: ", nome, "\n")
		escreva ("Idade: ", idade, "\n")
		escreva ("Sexo: ", sexo, "\n")
		escreva ("Salário líquido: ", sliquido)
		}
////////////////////////////////////////////////////////////////////////////////////////////
		//calculo do inss de R$3.858 à 7.508
		se (sbruto >= 3858 e sbruto <= 7508){
			inss = (sbruto * 14) / 100
			sliquido = sbruto - inss

			//Calculo do imposto de renda de R$1904 a R$2827
			se (sbruto >= 1904 e sbruto <= 2827){
				irrf = (sbruto * 7.5) / 100
				sliquido = sbruto - inss - irrf	
			}

			//calculo do imposto de renda de R$2.828 a R$3.751
			se (sbruto >= 2828 e sbruto <= 3751){
				irrf = (sbruto * 15) / 100
				sliquido = sbruto - inss - irrf	
			}

			//calculo do imposto de renda de R$3.752 a R$4.665
			se (sbruto >= 3752 e sbruto <= 4665){
				irrf = (sbruto * 22.5) / 100
				sliquido = sbruto - inss - irrf	
			}

			
			//calculo do imposto de renda de R$4666 para cima 
			se (sbruto >= 4666){
				irrf = (sbruto * 27.5) / 100
				sliquido = sbruto - inss - irrf	
			}
			
		escreva ("          INFORMAÇÕES FUNCIONÁRIOS       ", "\n")
		escreva ("Nome: ", nome, "\n")
		escreva ("Idade: ", idade, "\n")
		escreva ("Sexo: ", sexo, "\n")
		escreva ("Salário líquido: ", sliquido)
		}
		
	}
}
