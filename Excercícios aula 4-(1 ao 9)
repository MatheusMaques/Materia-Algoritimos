programa
{
	
//5-Ler 80 números e ao final informar quantos número(s) est(á)ão no intervalo entre 10 (inclusive) e 150 (inclusive). 
	funcao inicio()
	{
		//variaveis
		inteiro inicial, x
		inteiro result = 0
		
	 	para (inicial = 1; inicial < 80; inicial ++){
			escreva ("Digite um valor: ", "\n")
				leia (x)
						
		 	se (x >= 10 e x <= 150){
		 	result = result + 1
		 	}
		}
		escreva ("Existem ", result," números entre o intervalo de 10 e 150") 
	}
}

