programa
{
	
//2-Escrever um algoritmo que leia dois valores inteiro distintos e informe qual é o maior.
	funcao inicio()
	{
		//variaves
		inteiro x, y

		escreva (("Digite um valor: "), "\n")
			leia (x)
		escreva (("Digite um segundo valor: "), "\n")
			leia (y)

		//validação do maior valor
		se (x > y){
			escreva ("O valor ", x, " é maior que ", y) 
		}
		
		senao {
			escreva ("O valor ", y, " é maior que ", x)
		}
		
	}
}

